          "200": {
            "description": "OK"
          },
          "400": {
            "description": "Bad request"
          }
        },
        "content": {
          "application/json": {}
        },
        "parameters": [
          {
            "name": "q",
            "in": "query",
            "description": "Specifies the query string",
            "required": true,
